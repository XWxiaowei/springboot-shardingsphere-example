<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jay.mapper.nosharding.ShardConfigMapper">
  <resultMap id="BaseResultMap" type="com.jay.model.ShardConfig">
    <id column="config_key" jdbcType="VARCHAR" property="configKey" />
    <result column="config_value" jdbcType="VARCHAR" property="configValue" />
  </resultMap>
  <sql id="Base_Column_List">
    config_key, config_value
  </sql>
  <select id="selectByPrimaryKey" parameterType="java.lang.String" resultMap="BaseResultMap">
     select config_key, config_value
    from shard_config
    where config_key = #{configKey,jdbcType=VARCHAR}
  </select>
  <select id="selectByKey" parameterType="java.util.List" resultMap="BaseResultMap">
    select config_key, config_value
    from shard_config
    where config_key in 
    <foreach collection="keys" item="key" open="(" separator="," close=")">
        #{key}
    </foreach>
  </select>
</mapper>